{
  "schemaVersion": 1,
  "id": "modid",
  "version": "${version}",

  "name": "Archers' Arsenal",
  "description": "Archers' Arsenal is a vanilla enhancement mod that aims to give ranged combat weapons diverse options equal to vanilla melee weapons.",
  "authors": [
    "MK"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "GNU GPL v3.0",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.mk.archers_arsenal.ArchersArsenal"
    ]
  },
  "mixins": [
    "archers_arsenal.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.14",
    "fabric-api": "*",
    "minecraft": "~1.19.2",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
